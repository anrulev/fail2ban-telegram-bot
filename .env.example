# Токен Telegram бота (получите у @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# ID чата для отправки уведомлений
# Для личного чата: используйте @userinfobot чтобы узнать ваш ID
# Для группы: добавьте @RawDataBot в группу и посмотрите "chat":{"id":
TELEGRAM_CHAT_ID=your_chat_id_here

# Настройки мониторинга
# Интервал проверки лога в секундах
CHECK_INTERVAL=60

# Максимальное количество уведомлений за одну проверку
MAX_ALERTS_PER_RUN=10 